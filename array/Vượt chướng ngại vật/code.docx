#include <iostream>
using namespace std;

int dx[] = {-1,-1,-1, 0,0, 1,1,1};
int dy[] = {-1, 0, 1,-1,1,-1,0,1};

int main() {
	int T; cin >> T;
	for (int tc = 1; tc <= T; tc++){
		int n, x, y; cin >> n >> x >> y;
		int board[100][100];
		for(int i = 0; i < n; i++){
			for(int j = 0; j < n; j++){
				cin >> board[i][j];
			}
		}
		int count = 0;
		while(true){
			int min = 9999999;
			int tempx, tempy;
			for(int i = 0; i < 8; i++){
				int nx = x + dx[i];
				int ny = y + dy[i];
				if(nx < 0 || nx >= n || ny < 0 || ny >= n){
					continue;
				}
				if(board[nx][ny] > board[x][y] && board[nx][ny] < min){
					min = board[nx][ny];
					tempx = nx;
					tempy = ny;
				}
			}
			if(min == 9999999){
				break;
			}
			x = tempx;
			y = tempy;
			count ++;
		}
		cout << "#" << tc << " " << count << endl; 
	}
	

	return 0;
}