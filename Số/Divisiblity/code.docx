#include <iostream>
using namespace std;
#include<cmath>

int main() {
	int T; cin >> T;
	for (int tc = 0; tc < T; tc++){
	    int n,x,y;
	    cin >> n >> x >> y;
	    for(int i = x; i < n; i++){
	        if(i % x == 0 && i % y != 0){
	            cout << i << " ";
	        }
	    }cout << "\n";
	} cout << endl;
	
	return 0;
}