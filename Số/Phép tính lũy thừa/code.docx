#include <iostream>
using namespace std;
#include<math.h>

int main() {
	
	int n; cin >> n;
	int arr[n] = {0};
	int a, b;
	for (int i = 0; i < n; i++){
		cin >> a >> b;
		arr[i] += pow(a,b);
	} 
    for (int i = 0; i < n; i++){
        cout << "#" << (i+1) << " " << arr[i] << endl;
    }
	
	return 0;
}
